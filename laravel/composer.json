{
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.50"
    },
    "require-dev": {
        "laravel/dusk": "^7.5",
        "phpunit/phpunit": "^9.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
