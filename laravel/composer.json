{
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.8.*"
    },
    "require-dev": {
        "laravel/dusk": "^5.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
